  "components": {
    "schemas": {
      "ApplicationNoteDTO": {
        "required": [
          "content",
          "jobApplicationId",
          "type"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "jobApplicationId": {
            "type": "integer",
            "format": "int32"
          },
          "type": {
            "$ref": "#/components/schemas/NoteType"
          },
          "content": {
            "maxLength": 2000,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "ApplicationStatus": {
        "enum": [
          "INTERESTED",
          "APPLIED",
          "INTERVIEWING",
          "REJECTED",
          "OFFER"
        ],
        "type": "string"
      },
      "CompanyDTO": {
        "required": [
          "name",
          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "companyValues": {
            "maxLength": 500,
            "type": "string",
            "nullable": true
          },
          "salaryInfoGlassdoor": {
            "maximum": 99999,
            "minimum": 0,
            "type": "number",
            "format": "double",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ContactDTO": {
        "required": [
          "name",
          "notes",
          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "companyId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "linkedinUrl": {
            "type": "string",
            "nullable": true
          },
          "notes": {
            "maxLength": 500,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "JobApplicationDTO": {
        "required": [
          "applicationLink",
          "cvVersion",
          "jobDescription",
          "jobTitle",
          "status",
          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "companyId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "jobTitle": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "jobDescription": {
            "maxLength": 2000,
            "minLength": 1,
            "type": "string"
          },
          "applicationLink": {
            "minLength": 1,
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/ApplicationStatus"
          },
          "cvVersion": {
            "minLength": 1,
            "type": "string"
          },
          "appliedAt": {
            "type": "string",
            "format": "date-time",
            "readOnly": true
          }
        },
        "additionalProperties": false
      },
      "NoteType": {
        "enum": [
          "CHALLENGE",
          "QUESTION",
          "GENERAL"
        ],
        "type": "string"
      },
      "RoleDTO": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "userId": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int32"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UserLoginDTO": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "UserRegisterDTO": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 6,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "UserResponseDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "email": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
